{
  "name": "vscode-web-extension",
  "displayName": "a file system provider sample",
  "description": "description",
  "publisher": "wedbav",
  "version": "0.0.1",
  "private": true,
  "files": [
    "package.nls.json",
    "dist"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onFileSystem:wedbav"
  ],
  "main": "./extension.js",
  "browser": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "wedbav.reset",
        "title": "Delete Files",
        "category": "WEDBAV"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "wedbav.reset",
          "when": "workbenchState == workspace"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "fets": "^0.8.5",
    "js-base64": "^3.7.8"
  },
  "devDependencies": {
    "@types/node": "^22.18.8",
    "@types/vscode": "^1.104.0",
    "esbuild-loader": "^4.3.0",
    "esbuild-register": "^3.6.0",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  }
}
